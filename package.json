{
	"name": "spirits",
	"version": "3.0.0",
	"author": "McKayla Washburn <mckayla@hey.com>",
	"description": "Get the spirit of a string, without the whole thing!",
	"license": "MPL-2.0",
	"homepage": "https://mckay.la/spirits",
	"repository": "https://github.com/aslilac/spirits",
	"bugs": "https://github.com/aslilac/spirits/issues",
	"type": "module",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		".": {
			"import": "./build/index.js",
			"types": "./build/index.d.ts"
		}
	},
	"files": [
		"build/**"
	],
	"devDependencies": {
		"prettier": "~3.3.3",
		"typedoc": "^0.26.11",
		"typescript": "~5.6.3",
		"vitest": "^2.1.5"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"dev": "tsc -p tsconfig.build.json -w",
		"doc": "typedoc",
		"fmt": "prettier --write .",
		"prepack": "npm run build -s",
		"test": "vitest run",
		"type-check": "tsc -p . --noEmit"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
